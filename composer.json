{
    "name": "zuruh/discord-bot-bundle",
    "description": "Integration of team-reflex/discord-php with symfony",
    "type": "symfony-bundle",
    "keywords": ["symfony", "discord", "bundle", "bot"],
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Zuruh\\DiscordBotBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zuruh\\DiscordBotBundle\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Zuruuh",
            "email": "ziadi.mail.pro@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.2",
        "team-reflex/discord-php": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.5",
        "ekino/phpstan-banned-code": "^1.0",
        "killov/phpstan-banned-double-equals": "^1.0",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.14",
        "rector/rector": "^0.15.20"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
